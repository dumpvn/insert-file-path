{
  "name": "insert-file-path",
  "displayName": "Insert File Path",
  "description": "Simple vscode extesion to insert full file path",
  "version": "0.0.2",
  "publisher": "dumpvn",
  "repository": "https://github.com/dumpvn/insert-file-path",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "insert-file-path.helloWorld",
        "title": "Insert File Path"
      }
    ],
    "keybindings": [
      {
        "command": "insert-file-path.helloWorld",
        "key": "ctrl+alt+i",
        "mac": "shift+cmd+i",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
